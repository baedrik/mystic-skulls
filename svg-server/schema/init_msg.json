{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "InitMsg",
  "description": "Instantiation message",
  "type": "object",
  "required": [
    "entropy",
    "jaw_weight",
    "jawless_weight"
  ],
  "properties": {
    "entropy": {
      "description": "entropy used for prng seed",
      "type": "string"
    },
    "jaw_weight": {
      "description": "weight for jawed skulls",
      "type": "integer",
      "format": "uint16",
      "minimum": 0.0
    },
    "jawless_weight": {
      "description": "weight for jawless skulls",
      "type": "integer",
      "format": "uint16",
      "minimum": 0.0
    }
  }
}
