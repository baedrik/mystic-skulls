{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "HandleAnswer",
  "description": "Responses from handle functions",
  "anyOf": [
    {
      "description": "response from creating a viewing key",
      "type": "object",
      "required": [
        "viewing_key"
      ],
      "properties": {
        "viewing_key": {
          "type": "object",
          "required": [
            "key"
          ],
          "properties": {
            "key": {
              "type": "string"
            }
          }
        }
      }
    },
    {
      "description": "response from adding/removing admins",
      "type": "object",
      "required": [
        "admins_list"
      ],
      "properties": {
        "admins_list": {
          "type": "object",
          "required": [
            "admins"
          ],
          "properties": {
            "admins": {
              "type": "array",
              "items": {
                "$ref": "#/definitions/HumanAddr"
              }
            }
          }
        }
      }
    },
    {
      "description": "response from adding/removing viewers",
      "type": "object",
      "required": [
        "viewers_list"
      ],
      "properties": {
        "viewers_list": {
          "type": "object",
          "required": [
            "viewers"
          ],
          "properties": {
            "viewers": {
